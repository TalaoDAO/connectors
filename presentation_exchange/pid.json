{
  "id": "pid",
  "input_descriptors": [
    {
      "id": "pid_credential",
      "format": {
        "vc+sd-jwt": {
          "sd-jwt_alg_values": [
            "ES256",
          ],
          "kb-jwt_alg_values": [
            "ES256",
          ]
        }
      },
      "constraints": {
        "limit_disclosure": "required",
        "fields": [
          {
            "path": [
              "$.family_name"
            ]
          },
          {
            "path": [
              "$.given_name"
            ]
          },
          {
            "path": [
              "$.birth_date"
            ]
          },
          {
            "path": [
              "$.birth_place"
            ]
          },
          {
            "path": [
              "$.nationality"
            ]
          }
        ]
      }
    }
  ]
}